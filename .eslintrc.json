{
    "env": {
        "browser": true,
        "jquery": true
    },
    "extends": ["eslint:recommended", "plugin:jsdoc/recommended"],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": ["jsdoc"],
    "rules": { "quote-props": ["warn", "consistent"] },
    "globals": {
        "DB": "writable",
        "BeveragesDB": "writable",
        "DatabaseAPI": "writable",
        "Dictionary": "writable"
    }
}
