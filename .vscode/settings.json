{
    "cSpell.diagnosticLevel": "Hint",
    "psi-header.changes-tracking": {
        "isActive": true,
        "include": [],
        "includeGlob": ["**/*.js", "**/*.html", "**/*.css"],
        "exclude": [],
        "excludeGlob": [
            "./**/*/ignoreme.*",
            "**/BeveragesDB.js",
            "**/DBLoaded.js"
        ],
        "autoHeader": "autoSave",
        "enforceHeader": true
    },
    "psi-header.templates": [
        {
            "language": "*",
            "template": [
                "File: <<filename>>",
                "",
                "DESCRIPTION",
                "",
                "Author: <<author>>"
            ]
        }
    ]
}
